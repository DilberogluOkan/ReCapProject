@using Entities.DTOs
@model List<CarDetailDto>
@{
    ViewBag.Title = "Home Page";
}


<table class="table table-bordered" style="margin-top:20px">

    <tr>
        <th>Id</th>
        <th>Araç Adı</th>
        <th>Renk</th>
        <th>Ücreti</th>
    </tr>
    @foreach (var k in Model)
    {
        <tr>
            <td>@k.Id</td>
            <td>@k.CarName</td>
            <td>@k.ColorName</td>
            <td>@k.DailyPrice</td>
        </tr>
    }
</table>
<a href="#" class="btn btn-danger" >Ekle</a>

